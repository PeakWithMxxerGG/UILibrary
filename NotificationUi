local Noti = {}local function a()local b=Instance.new("ScreenGui")b.Name="SigmaX"b.Parent=game.CoreGui local c=Instance.new("Frame")c.Name="MainHolder"c.Parent=b c.AnchorPoint=Vector2.new(0,1)c.BackgroundColor3=Color3.fromRGB(255,255,255)c.BackgroundTransparency=1 c.Position=UDim2.new(0,25,1,-25)c.Size=UDim2.new(0,300,1,-25)local d=Instance.new("UIListLayout")d.Parent=c d.HorizontalAlignment=Enum.HorizontalAlignment.Left d.SortOrder=Enum.SortOrder.LayoutOrder d.Padding=UDim.new(0,5)return c end local function e(f,g,h,i)local j=Instance.new("Frame")j.Name="Sigma"j.Parent=f j.BackgroundColor3=Color3.fromRGB(25,25,25)j.BorderSizePixel=0 j.Position=UDim2.new(1,0,0,0)j.Size=UDim2.new(1,0,0,60)local k=Instance.new("UICorner")k.CornerRadius=UDim.new(0,6)k.Parent=j local l=Instance.new("TextLabel")l.Name="Title"l.Parent=j l.BackgroundTransparency=1 l.Position=UDim2.new(0,15,0,8)l.Size=UDim2.new(1,-15,0,15)l.Font=Enum.Font.GothamBold l.Text=g l.TextColor3=Color3.fromRGB(255,255,255)l.TextSize=14 l.TextXAlignment=Enum.TextXAlignment.Left local m=Instance.new("TextLabel")m.Name="Text"m.Parent=j m.BackgroundTransparency=1 m.Position=UDim2.new(0,15,0,25)m.Size=UDim2.new(1,-15,0,30)m.Font=Enum.Font.GothamMedium m.Text=h m.TextColor3=Color3.fromRGB(200,200,200)m.TextSize=12 m.TextWrapped=true m.TextXAlignment=Enum.TextXAlignment.Left j.Position=UDim2.new(-1,-300,0,0)local n=game:GetService("TweenService"):Create(j,TweenInfo.new(0.5,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{Position=UDim2.new(0,0,0,0)})n:Play()task.spawn(function()task.wait(i or 5)local o=game:GetService("TweenService"):Create(j,TweenInfo.new(0.5,Enum.EasingStyle.Quart,Enum.EasingDirection.Out),{Position=UDim2.new(-1,-300,0,0)})o:Play()task.wait(0.5)j:Destroy()end)end Noti.Init=a Noti.Notify=e return Noti
